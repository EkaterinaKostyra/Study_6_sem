   "source": [
    "def true_fun(x):\n",
    "    return 10 * np.cos(1.5 * np.sin(x))\n",
    "\n",
    "n_samples = 150\n",
    "\n",
    "X = np.sort(np.random.rand(n_samples))\n",
    "y = true_fun(X) + np.random.randn(n_samples) * 0.5"
   ]
  },
